    alias df='df -h' 
    alias free='free -m'
    alias cp="cp -i"                                               
    alias gitu='git add . && git commit && git push'
    alias bashrc='vim ~/.bashrc && source ~/.bashrc'
	alias du='ncdu'
	alias mail='mutt'
	alias lsl='ls -gl | less'
	alias zshrc='vim ~/.zshrc && source ~/.zshrc'
	alias nonsa='chromium nonsa.pl &'
	alias cc='g++ -Werror -o'
	alias apt='sudo pacman -S'
	alias pacup='sudo pacman -Syu'
	alias v='vim'
	alias su='su -l'
	alias mkdir='mkdir -pv'
	alias sha1='sha1sum'
	alias sha512='sha512sum'
	alias start_bar='killall polybar ; polybar -r openbox-bar >/tmp/log.polybar 2>/tmp/error.polybar &'
    alias i3conf='vim ~/.config/i3/config' 
	alias obout='oblogout'
	alias grubup='sudo update-grub'
	alias ls='ls --color=auto'
	alias Xres='vim ~/.Xresources && xrdb ~/.Xresources'
